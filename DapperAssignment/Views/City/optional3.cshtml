@{
}

@model List<Country>
@{
    ViewBag.Title = "EU countries with life expectancy";
}
<h2>EU countries with life expectancy</h2>

<table>
    <tr>
        <td>Name</td>
        <td>Continent</td>
        <td>LifeExpectancy</td>
    </tr>

    @foreach (Country country in @Model)
    {
        <tr>
            <td>@country.Name</td>
            <td>@country.Continent</td>
            <td>@country.LifeExpectancy</td>
        </tr>
    }
</table>