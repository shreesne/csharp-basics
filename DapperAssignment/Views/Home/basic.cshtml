@model List<City>
@{

    ViewBag.Title = "List of cities with population span";
}
<h2>List of cities with population span</h2>

<table>
    <tr>
        <td>Id </td>
        <td>Name</td>
        <td>CountryCode</td>
        <td>District</td>
        <td>Population</td>

    </tr>

    @foreach (City city in @Model)
    {
        <tr>
            <td>@city.Id </td>
            <td>@city.Name</td>
            <td>@city.CountryCode</td>
            <td>@city.District</td>
            <td>@city.Population</td>
        </tr>
    }

</table>